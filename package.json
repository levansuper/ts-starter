{
	"name": "kyc-gateway",
	"version": "0.0.1",
	"scripts": {
		"start": "tsc && node dist/app.js",
		"start:dev": "yarn install && gulp build-reload",
		"start:watch": "yarn install && gulp watch",
		"clean": "gulp clean",
		"precommit": "lint-staged",
		"prettier": "pretty-quick"
	},
	"lint-staged": {
		"*.{ts,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"prettier": {
		"useTabs": true,
		"semi": true,
		"singleQuote": true
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kyc-chain/kyc-monax.git"
	},
	"bugs": {
		"url": "https://github.com/kyc-chain/kyc-monax/issues"
	},
	"dependencies": {
		"@types/express-session": "^1.15.10",
		"connect-redis": "3.3.3",
		"cookie-parser": "1.4.3",
		"crypto": "^1.0.1",
		"dotenv": "6.0.0",
		"express": "4.16.3",
		"express-session": "1.15.6",
		"fs-extra": "3.0.1",
		"mongoose": "5.1.6",
		"source-map-support": "^0.5.6"
	},
	"devDependencies": {
		"@types/dotenv": "4.0.3",
		"@types/express": "4.16.0",
		"@types/lodash": "4.14.110",
		"@types/mongoose": "5.0.18",
		"@types/node": "8.0.28",
		"gulp": "3.9.1",
		"gulp-clean": "0.4.0",
		"gulp-jasmine": "4.0.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-typescript": "5.0.0-alpha.2",
		"husky": "^0.14.3",
		"lint-staged": "^7.2.0",
		"prettier": "1.13.6",
		"pretty-quick": "1.6.0",
		"request": "2.83.0",
		"run-sequence": "2.2.1",
		"typescript": "2.9.2"
	}
}
